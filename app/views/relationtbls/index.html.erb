<p id="notice"><%= notice %></p>

<h1>Listing Relationtbls</h1>

<table>
  <thead>
    <tr>
      <th>Relationid</th>
      <th>Frommvid</th>
      <th>Tomvid</th>
      <th>Fromtitle</th>
      <th>Totitle</th>
      <th>Updatedate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @relationtbls.each do |relationtbl| %>
      <tr>
        <td><%= relationtbl.relationid %></td>
        <td><%= relationtbl.frommvid %></td>
        <td><%= relationtbl.tomvid %></td>
        <td><%= relationtbl.fromtitle %></td>
        <td><%= relationtbl.totitle %></td>
        <td><%= relationtbl.updatedate %></td>
        <td><%= link_to 'Show', relationtbl %></td>
        <td><%= link_to 'Edit', edit_relationtbl_path(relationtbl) %></td>
        <td><%= link_to 'Destroy', relationtbl, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Relationtbl', new_relationtbl_path %>
